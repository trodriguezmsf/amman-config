                    },
                    "expandedViewConfig": {
                        "conceptNames": [
                            "Consultation Note"
                        ],
                        "isEditable": true,
                        "showDetailsButton": true,
                        "pivotTable": {}
                    }
                },
                "surgicalHistory": {
                    "type": "custom",
                    "displayOrder": 7,
                    "displayType": "Full-Page",
                    "config": {
                        "title": "DASHBOARD_TITLE_SURGICAL_APPOINTMENTS_KEY",
                        "template": "<surgical-history section='section' patient='patient'></surgical-history>"
                    }
                },
                "patientMovementHistory": {
                    "type": "custom",
                    "displayType": "Full-Page",
                    "displayOrder": 20,
                    "config": {
                        "title": "DASHBOARD_TITLE_PATIENT_MOVEMENT_HISTORY_KEY",
                        "template": "<patient-movement-history section='section' patient='patient' visitUuid='visitUuid'></patient-movement-history>"
                    }
                }
            }
