{
    "id": "bahmni.ipd.admission",
    "extensionPoints": [
        {
            "id": "org.bahmni.ipd.admit.action",
            "description": "Bahmni Patient Admission Action"
        },
        {
            "id": "org.bahmni.ipd.transfer.action",
            "description": "Bahmni Patient Transfer Action"
        },
        {
            "id": "org.bahmni.ipd.discharge.action",
            "description": "Bahmni Patient Discharge Action"
        },
        {
            "id": "org.bahmni.patient.search",
            "description": "Bahmni patient search, lookup Page"
        }
    ],
    "contextModel": [
        "patientUuid",
        "encounterUuid",
        "visitUuid"
    ],
    "config": {
        "wardListPrintEnabled": true,
        "wardListPrintViewTemplateUrl": "/bahmni_config/openmrs/apps/ipd/wardListPrint.html",
        "wardListPrintAttributes":["Bed Tags","Bed Number","Patient ID","Patient Name","Age","Admission Date","Expected Date of Discharge",
            "Speciality","Name of Surgeon","Date of Surgery","Surgery","Dressing Frequency", "Recent Lab Results","Is Caretaker Required?",
            "Caretaker Gender","Date of Appointment","Slot","Provider name"],
        "wardListSqlSearchHandler": "emrapi.sqlGet.allWardsListDetails",
        "diagnosisStatus": "Inactive",
        "onAdmissionForwardTo": "#/bedManagement/patient/{{patientUuid}}/visit/{{visitUuid}}",
        "onTransferForwardTo": "#/bedManagement/patient/{{patientUuid}}/visit/{{visitUuid}}",
        "onDischargeForwardTo": "#/bedManagement/bed/{{bedId}}",
        "defaultVisitType": "Hospital",
        "expectedDateOfDischarge": "IPD Expected DD",
        "maxPatientsPerBed": 2,
        "conceptSetUI": {
            "Expected Date of Discharge": {
                "disableAddNotes": true,
                "allowFutureDates": true
            }
        },
        "dashboard": {
            "translationKey": "DASHBOARD_TAB_GENERAL_KEY",
            "conceptName": "Adt Notes",
            "sections": {
                "patientInformation": {
                    "translationKey": "DASHBOARD_TITLE_PATIENT_INFORMATION_KEY",
                    "type": "patientInformation",
                    "displayOrder": 0,
                    "patientAttributes": [
                        "caste",
                        "class",
                        "education",
                        "occupation",
                        "debt",
                        "distanceFromCenter",
                        "isUrban",
                        "cluster",
                        "RationCard",
                        "familyIncome"
                    ],
                    "addressFields": [
                        "address1",
                        "address2",
                        "cityVillage",
                        "countryDistrict"
                    ]
                },
                "navigationLinksControl": {
                    "translationKey": "DASHBOARD_TITLE_NAVIGATION_LINKS_CONTROL_KEY",
                    "type": "navigationLinksControl",
                    "displayOrder": 1,
                    "showLinks": [
                        "home",
                        "registration",
                        "enrolment"
                    ],
                    "customLinks": [
                        {
                            "name": "bedManagement",
                            "translationKey": "PATIENT_BED_MANAGEMENT_PAGE_KE",
                            "url": "../bedmanagement/#/bedManagement/patient/{{patientUuid}}/visit/{{visitUuid}}"
                        }
                    ]
                },
                "admissionDetails": {
                    "translationKey": "DASHBOARD_TITLE_ADMISSION_DETAILS_KEY",
                    "type": "admissionDetails",
                    "displayOrder": 4,
                    "expandedViewConfig": {
                        "numberOfVisits": 3,
                        "showDetailsButton": true,
                        "pivotTable": {}
                    }
                },
                "disposition": {
                    "translationKey": "DASHBOARD_TITLE_DISPOSITION_KEY",
                    "type": "disposition",
                    "numberOfVisits": 6,
                    "displayOrder": 3
                },
                "nursingnote": {
                    "translationKey": "DASHBOARD_TITLE_WARD_NURSING_NOTE_KEY",
                    "type": "vitals",
                    "isObservation": true,
                    "displayOrder": 8,
                    "dashboardConfig": {
                        "showDetailsButton": true,
                        "isEditable": true,
                        "conceptNames": [
                            "Ward Nursing Note"
                        ],
                        "showGroupDateTime": true
                    },
                    "expandedViewConfig": {
                        "conceptNames": [
                            "Ward Nursing Note"
                        ],
