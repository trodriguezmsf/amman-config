                        "conceptNames": [
                            "Physician Progress Note-Ward"
                        ],
                        "showGroupDateTime": true
                    },
                    "expandedViewConfig": {
                        "conceptNames": [
                            "Physician Progress Note-Ward"
                        ],
                        "isEditable": true,
                        "showDetailsButton": true,
                        "pivotTable": {}
                    }
                },
                "complications": {
                    "translationKey": "DASHBOARD_COMPLICATIONS_KEY",
                    "type": "vitals",
                    "isObservation": true,
                    "displayOrder": 12,
                    "dashboardConfig": {
                        "showDetailsButton": true,
                        "isEditable": true,
