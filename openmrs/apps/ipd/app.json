                "consultationnote": {
                    "translationKey": "DASHBOARD_CONSULTATION_NOTE_KEY",
                    "type": "vitals",
                    "isObservation": true,
                    "displayOrder": 13,
                    "dashboardConfig": {
                        "showDetailsButton": true,
                        "isEditable": true,
                        "conceptNames": [
                            "Consultation Note"
